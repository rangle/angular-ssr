{
  "name": "angular-ssr",
  "version": "0.0.0",
  "description": "Angular server-side rendering implementation",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.10"
  },
  "bin": {
    "ng-render": "bin/render.js"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "watch": "tsc --watch",
    "pretest": "npm run lint",
    "lint": "tslint --project tsconfig.json --type-check",
    "test": "jest --verbose",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clbond/angular-ssr.git"
  },
  "contributors": [
    {
      "name": "Christopher Bond",
      "email": "cb@clbond.org",
      "url": "https://clbond.org/"
    },
    {
      "name": "rangle.io",
      "url": "https://rangle.io/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clbond/angular-ssr/issues"
  },
  "peerDependencies": {
    "@angular/common": "^2.4",
    "@angular/compiler": "^2.4",
    "@angular/compiler-cli": "^2.4",
    "@angular/core": "^2.4",
    "@angular/router": "^3.4",
    "reflect-metadata": "^0.1",
    "zone.js": "^0.7",
    "typescript": "^2.2.0"
  },
  "optionalPeerDependencies": {
    "@ngrx/store": "^2.2.1"
  },
  "dependencies": {
    "@angular/compiler-cli": "^2.4.7",
    "@angular/tsc-wrapped": "^0.5.2",
    "@types/babel-core": "6.7.14",
    "@types/codependency": "0.1.1",
    "@types/jasmine": "2.5.41",
    "babel-core": "6.23.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "codependency": "0.1.4",
    "commander": "2.9.0",
    "domino": "git://github.com/clbond/domino.git#0c16ed953edf3a9a741b435e7359f6acfe2363bb",
    "mock-local-storage": "^1.0.2",
    "reflect-metadata": "0.1.9",
    "rimraf": "2.5.4",
    "rxjs": "5.1.0",
    "scoped-logger": "^0.0.19",
    "source-map-support": "0.4.11",
    "typescript": "2.2.0"
  },
  "devDependencies": {
    "@angular/common": "^2.4.6",
    "@angular/compiler": "^2.4.6",
    "@angular/core": "^2.4.6",
    "@angular/platform-browser": "^2.4.7",
    "@angular/router": "^3.4.6",
    "@ngrx/store": "^2.2.1",
    "jasmine-core": "2.5.2",
    "jasmine-promises": "0.4.1",
    "jest": "19.0.2",
    "ts-jest": "^19.0.0",
    "tslint": "4.4.2",
    "zone.js": "0.7.6"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "modulePaths": [
      "<rootDir>/source"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/source/test/fixtures/dependencies.js",
    "modulePathIgnorePatterns": [
      "bundles(\\|/)"
    ],
    "transform": {
      "@angular/(.*)\\.js$": "<rootDir>/build/transpile/process.js",
      "\\.ts$": "ts-jest/dist/preprocessor.js"
    },
    "moduleNameMapper": {
      "@angular/([^/]+)$": "@angular/$1/index"
    },
    "transformIgnorePatterns": [],
    "testRegex": "source/.*/tests/.*\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "files": [
    "bin",
    "build",
    "package.json",
    "source",
    "tsconfig.json",
    "tslint.json"
  ]
}
